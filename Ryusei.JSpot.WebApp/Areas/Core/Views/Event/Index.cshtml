@{
    Layout = null;
    string eventId = (string)Model;
}

<!-- ============================================================== -->
<!-- Temp - Earnings -->
<!-- ============================================================== -->
<div class="card gredient-info-bg mt-0 mb-0">
    <div class="card-body">
        <h3 class="card-title text-white" data-elysium-event-name></h3>
        <h4 class="card-subtitle text-white op-5" data-elysium-event-description></h4>
        <div class="row mt-4 mb-3">
            <!-- col -->
            <div class="col-sm-12 col-lg-4">
                <div class="temp d-flex align-items-center flex-row">
                    <div class="display-5 text-white"><i class="fas fa-calendar"></i></div>
                    <div class="ml-2" >
                        <h4 class="mb-0 text-white" data-elysium-event-start-date></h4><small class="text-white op-5">Fecha de inicio</small>
                    </div>
                </div>
            </div>
            <div class="col-sm-12 col-lg-4">
                <div class="temp d-flex align-items-center flex-row">
                    <div class="display-5 text-white"><i class="fas fa-calendar"></i></div>
                    <div class="ml-2" >
                        <h4 class="mb-0 text-white" data-elysium-event-end-date></h4><small class="text-white op-5">Fecha de terminación</small>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ============================================================== -->
<!-- Earnings -->
<!-- ============================================================== -->
<div class="row">
    <div class="col-sm-12">
        <ul class="nav nav-pills custom-pills" id="pills-tab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="pills-home-tab2" data-toggle="pill" href="#addresspanel" role="tab" aria-selected="true">Dirección</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-home-tab2" data-toggle="pill" href="#eventgrouppanel" role="tab" aria-selected="true">Grupos</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-profile-tab2" data-toggle="pill" href="#transportpanel" role="tab" aria-selected="false">Transporte</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-contact-tab2" data-toggle="pill" href="#assistantpanel" role="tab" aria-selected="false">Asistentes</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-session-tab2" data-toggle="pill" href="#invitationspanel" role="tab" aria-selected="false">Invitaciones</a>
            </li>
        </ul>
        <div class="tab-content mt-4" id="pills-tabContent">
            <!-- Address panel -->
            <div class="tab-pane fade show active" id="addresspanel" role="tabpanel">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="card">
                            
                            <div class="card-body">
                                <div class="d-flex no-block align-items-center mb-3">
                                    <span><i class=" fas fa-map-marker-alt"></i> Dirección</span>
                                </div>
                                <p class="mb-0 mt-2 font-bold">Calle</p>
                                <p class="mb-0 mt-2" data-elysium-event-address-street>1ERA CERRADA XONACATEPEC</p>
                                <p class="mb-0 mt-2 font-bold">Número externo - interno</p>
                                <p class="mb-0 mt-2" data-elysium-event-address-numbers>103</p>
                                <p class="mb-0 mt-2 font-bold">Colonia</p>
                                <p class="mb-0 mt-2" data-elysium-event-address-neighborhood>Bosques de San Sebastian</p>
                                <p class="mb-0 mt-2 font-bold">Ciudad/Estado/Pais</p>
                                <p class="mb-0 mt-2" data-elysium-event-address-country>Puebla, Puebla, México</p>
                                <p class="mb-0 mt-2 font-bold">Código Postal</p>
                                <p class="mb-0 mt-2" data-elysium-event-address-zipcode>72310</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="card">
                            <div id="AddressMap" class="gmaps" style="height:450px; z-index: 1;"></div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Event Group Panel -->
            <div class="tab-pane fade " id="eventgrouppanel" role="tabpanel">
                <!-- Panel Areas -->
                <div class="row" data-elysium-event-area-selection-panel>
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <form data-elysium-event-area-selection-form>
                                    <div class="form-group">
                                        <label>Seleccione el área a la que pertenece</label>
                                        <select data-elysium-event-area-selection-select
                                                name="Areas"
                                                class="form-control"
                                                multiple ="multiple"
                                                required
                                                data-parsley-required-message="Debe seleccionar al menos un área a la que pertenece"
                                                ></select>
                                    </div>
                                    <button type="submit" class="btn btn-primary">Seleccionar</button>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- Panel Groups -->
                <div class="row" data-elysium-event-eventgroup-panel>
                    <div class="col-sm-12 col-lg-12">
                        <!-- Groups List -->
                        <div class="row">
                            <div class="col-sm-12 col-lg-4">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Grupos</h4>
                                        <div class="feed-widget scrollable" style="height:450px;">
                                            <ul class="list-style-none feed-body mt-5" data-elysium-event-eventgroup-list>
                                                
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-12 col-lg-8">
                                <div class="card">
                                    <div class="card-body" data-elysium-event-eventgroup-paneldetails>
                                        <ul class="nav nav-tabs" role="tablist">
                                            <li class="nav-item"> 
                                                <a data-elysium-event-eventgroup-groupinformationtab class="nav-link active show" data-toggle="tab" href="#groupdetailspanel" role="tab" aria-selected="true">
                                                    <span class="hidden-sm-up"><i class="fa fa-list-alt"></i></span>
                                                    <span class="hidden-xs-down">Información del grupo</span>
                                                </a> 
                                            </li>
                                            <li class="nav-item"> 
                                                <a class="nav-link show" data-toggle="tab" href="#assistantgrouppanel" role="tab" aria-selected="false">
                                                    <span class="hidden-sm-up"><i class="fas fa-users"></i></span> 
                                                    <span class="hidden-xs-down">Asistentes</span>
                                                </a> 
                                            </li>
                                        </ul>
                                        <!-- Tab panes -->
                                        <div class="tab-content tabcontent-border">
                                            <div class="tab-pane active" id="groupdetailspanel" role="tabpanel">
                                                <br />
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        <label>Nombre del grupo</label>
                                                        <p data-elysium-event-eventgroup-name></p>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <label>&nbsp;</label>
                                                        <button class="btn btn-primary" data-elysium-event-eventgroup-signin>Inscribirse</button>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label>Descripción</label>
                                                        <p data-elysium-event-eventgroup-description></p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        <label>Fecha de inicio</label>
                                                        <p data-elysium-event-eventgroup-startdate></p>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <label>Fecha de terminación</label>
                                                        <p data-elysium-event-eventgroup-enddate></p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        <label>Cupo</label>
                                                        <p data-elysium-event-eventgroup-capacity></p>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <label>Cupo disponible</label>
                                                        <p data-elysium-event-eventgroup-spots>5</p>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="tab-pane  p-20" id="assistantgrouppanel" role="tabpanel">
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        <label>Busqueda</label>
                                                        <input type="text" class="form-control ryusei-text-uppercase" data-elysium-event-eventgroup-participants-searchinput />
                                                    </div>
                                                </div>
                                                <br />
                                                <div class="row">
                                                    <div class="col-md-8" >
                                                        <div class="feed-widget scrollable" style="height:450px;" data-elysium-event-eventgroup-participants-list></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!-- Transport Panel -->
            <div class="tab-pane fade" id="transportpanel" role="tabpanel">
                <div class="row" data-elysium-event-transport-panel>
                    <div class="col-sm-12 col-lg-12">
                        <div class="card">
                            <div class="card-body">


                                <ul class="nav nav-tabs" role="tablist">
                                    <li class="nav-item"> 
                                        <a class="nav-link active show" data-toggle="tab" href="#transportgopanel" role="tab" aria-selected="true">
                                            <span class="hidden-sm-up"><i class="fas fa-car"></i></span>
                                            <span class="hidden-xs-down">Transporte de ida</span>
                                        </a> 
                                    </li>
                                    <li class="nav-item"> 
                                        <a class="nav-link show" data-toggle="tab" href="#transportreturnpanel" role="tab" aria-selected="false">
                                            <span class="hidden-sm-up"><i class="fas fa-car"></i></span> 
                                            <span class="hidden-xs-down">Transporte de vuelta</span>
                                        </a> 
                                    </li>
                                </ul>
                                <!-- Tab panes -->
                                <div class="tab-content tabcontent-border">

                                    <div class="tab-pane active" id="transportgopanel" role="tabpanel">
                                        <br />
                                        <div class="row">
                                            <div class="col-md-12">
                                                <button class="btn btn-info pull-right" data-elysium-event-transport-button-go>
                                                    Agregar auto
                                                </button>
                                            </div>
                                        </div>
                                        <div class="table-responsive mt-5">
                                            <table class="table table-striped table-bordered" id="TransportTableGo">
                                                <thead>
                                                    <tr>
                                                        <th>Icon</th>
                                                        <th>Modelo</th>
                                                        <th>Color</th>
                                                        <th>Cupo</th>
                                                        <th>Lugar de <br/>Salida</th>
                                                        <th>Fecha de <br />Salida</th>
                                                        <th>Agregarme</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>

                                    <div class="tab-pane" id="transportreturnpanel" role="tabpanel">
                                        <br />
                                        <div class="row">
                                            <div class="col-md-12">
                                                <button class="btn btn-info pull-right" data-elysium-event-transport-button-return>
                                                    Agregar auto
                                                </button>
                                            </div>
                                        </div>

                                        <div class="table-responsive mt-5">
                                            <table class="table table-striped table-bordered" id="TransportTableReturn">
                                                <thead>
                                                    <tr>
                                                        <th>Icon</th>
                                                        <th>Modelo</th>
                                                        <th>Color</th>
                                                        <th>Cupo</th>
                                                        <th>Lugar de <br />Salida</th>
                                                        <th>Fecha de <br />Salida</th>
                                                        <th>Agregarme</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <!-- Panel Form - Add -->
                <div class="row" data-elysium-event-transport-panel-form>
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <form data-elysium-event-transport-addform>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <!-- CarId -->
                                            <div class="form-group">
                                                <label>Seleccionar auto</label>
                                                <select id="CarId"
                                                        name="CarId"
                                                        class="form-control"
                                                        data-parsley-select-custom
                                                        data-parsley-select-custom-message="Debe ingresar el modelo de su auto"
                                                        ></select>

                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <!-- Departure date -->
                                            <div class="form-group">
                                                <label>Fecha y hora de partida</label>
                                                <input class="form-control"
                                                       id="DepartureDate"
                                                       name="DepartureDate"
                                                       required
                                                       data-parsley-required-message="Debe seleccionar una fecha de salida"
                                                        />
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Lugar de salida/llegada</label>
                                                <input class="form-control ryusei-text-uppercase"
                                                       id="Description"
                                                       name="Description"
                                                       required
                                                       data-parsley-required-message="Debe colocar una descripción del lugar de salida/llegada"
                                                       data-parsley-minlength="5"
                                                       data-parsley-minlength-message="La longitud minima es de 5 caracteres"
                                                       data-parsley-maxlength="250"
                                                       data-parsley-maxlength-message="La longitud maxima es de 250 caracteres"
                                                       data-elysium-iform-uppercase  />
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-md-6">
                                            <!-- Departure -->
                                            <div class="form-group">
                                                <label>Punto de partida</label>
                                                <div id="DepartureMap" class="gmaps" style="height:300px; z-index: 1;"></div>
                                                <div class="row" style="margin-bottom: 0">
                                                    <div class="col-md-12">
                                                        <select id="DepartureMapValidation"
                                                                name="DepartureMapValidation"
                                                                data-parsley-departuremarker-set
                                                                data-parsley-departuremarker-set-message="Debe seleccionar un punto de partida"
                                                                style="display:none">
                                                            <option value="1">1</option>
                                                            <option value="2">2</option>
                                                        </select>
                                                        <!--<p style="margin-bottom: 0" data-elysium-jspot-home-event-marker></p>-->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <!-- Arrive -->
                                            <div class="form-group">
                                                <label>Punto de llegada</label>
                                                <div id="ArriveMap" class="gmaps" style="height:300px; z-index: 1;"></div>
                                                <div class="row" style="margin-bottom: 0">
                                                    <div class="col-md-12">
                                                        <select id="ArriveMapValidation"
                                                                name="ArriveMapValidation"
                                                                data-parsley-arrivemarker-set
                                                                data-parsley-arrivemarker-set-message="Debe seleccionar un punto de partida"
                                                                style="display:none">
                                                            <option value="1">1</option>
                                                            <option value="2">2</option>
                                                        </select>
                                                        <!--<p style="margin-bottom: 0" data-elysium-jspot-home-event-marker></p>-->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class=" pull-right">
                                                <button class="btn btn-primary" type="submit">
                                                    <span class="fas fa-save"></span> Guardar
                                                </button> 
                                                <button class="btn btn-default" type="button" data-elysium-event-transport-button-cancel>
                                                    <span class="fas fa-times"></span> Cancelar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <!-- Assistant Panel -->
            <div class="tab-pane fade" id="assistantpanel" role="tabpanel">
                <div class="row" data-elysium-event-assistants-panel>
                    <div class="col-sm-12 col-lg-12">
                        <div class="row">
                            <div class="col-md-8">
                                <div class="card">
                                    <div class="card-body">
                                        <label>Busqueda</label>
                                        <input type="text" class="form-control ryusei-text-uppercase" data-elysium-event-assistants-input-search />
                                        <br />
                                        <div class="feed-widget scrollable" style="height:450px;" data-elysium-event-assistants-list></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="card border-left border-info">
                                    <div class="card-body">
                                        <div class="d-flex no-block align-items-center">
                                            <div>
                                                <span class="text-info display-6"><i class="ti-user"></i></span>
                                            </div>
                                            <div class="ml-auto">
                                                <h2 data-elysium-event-assistants-quantity></h2>
                                                <h6 class="text-info">Asistentes</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
            <!-- Transport Panel -->
            <div class="tab-pane fade" id="invitationspanel" role="tabpanel">
                <div class="row" data-elysium-event-invitation-panel>
                    <div class="col-sm-12 col-lg-12">
                        
                        <div class="row">
                            <div class="col-md-8">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <label>Busqueda</label>
                                                <input type="text" class="form-control ryusei-text-uppercase" data-elysium-event-invitation-input />
                                                <br />
                                                <div class="feed-widget scrollable" style="height:450px;" data-elysium-event-invitations-list></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">

                                <div class="card border-left border-success">
                                    <div class="card-body">
                                        <div class="d-flex no-block align-items-center">
                                            <div>
                                                <span class="text-success display-6"><i class="fas fa-check"></i></span>
                                            </div>
                                            <div class="ml-auto">
                                                <h2 data-elysium-event-invitations-accepted></h2>
                                                <h6 class="text-success">Aceptadas</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="card border-left border-danger">
                                    <div class="card-body">
                                        <div class="d-flex no-block align-items-center">
                                            <div>
                                                <span class="text-danger display-6"><i class="fas fa-times"></i></span>
                                            </div>
                                            <div class="ml-auto">
                                                <h2 data-elysium-event-invitations-rejected></h2>
                                                <h6 class="text-danger">Rechazadas</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="card border-left border-primary">
                                    <div class="card-body">
                                        <div class="d-flex no-block align-items-center">
                                            <div>
                                                <span class="text-primary display-6"><i class="fas fa-question"></i></span>
                                            </div>
                                            <div class="ml-auto">
                                                <h2 data-elysium-event-invitations-pending></h2>
                                                <h6 class="text-primary">Pendientes</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="card">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <form class="mt-4" data-elysium-event-invitations-form>
                                                    <label>Cargar invitaciones</label>
                                                    <div class="input-group">
                                                        <div class="custom-file">
                                                            <input type="file"
                                                                   class="custom-file-input"
                                                                   data-elysium-event-invitations-input
                                                                   accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
                                                                   required=""
                                                                   data-parsley-required-message="Debe seleecionar algun archivo a importar"
                                                                   data-parsley-max-file-size="5"
                                                                   data-parsley-max-file-size-message="El tamaño maximo es de 5MB"
                                                                   data-parsley-extension-valid="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
                                                                   data-parsley-extension-valid-message="La extension del archivo es incorrecta debe ser un archivo Excel 2007 o superior">
                                                            <label class="custom-file-label" data-elysium-event-invitations-label>Escoger Archivo</label>



                                                        </div>
                                                        <div class="input-group-append">
                                                            <button class="btn btn-outline-secondary" type="submit" data-elysium-event-invitations-submit>Enviar</button>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row" style="margin-bottom: 0">
                                                        <div class="col-md-12">
                                                            <p style="margin-bottom: 0" data-elysium-event-invitations-error></p>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



<div id="modalTransportDetail" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalles de transporte</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="card bg-info">
                            <div class="card-body text-white">
                                <div class="d-flex flex-row">
                                    <div class="display-6 align-self-center"><i class="fas fa-car"></i></div>
                                    <div class="p-10 align-self-center">
                                        <h4 class="mb-0">Cupo</h4>
                                        <span>Transporte</span>
                                    </div>
                                    <div class="ml-auto align-self-center">
                                        <h2 class="font-medium mb-0" data-elysium-event-modal-spots>0</h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card bg-success">
                            <div class="card-body text-white">
                                <div class="d-flex flex-row">
                                    <div class="display-6 align-self-center"><i class="fas fa-map-marker"></i></div>
                                    <div class="p-10 align-self-center">
                                        <h4 class="mb-0">Disponible</h4>
                                        <span>Transporte</span>
                                    </div>
                                    <div class="ml-auto align-self-center">
                                        <h2 class="font-medium mb-0" data-elysium-event-modal-spots-available>0</h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
