@{
    Layout = null;
}
<!-- ============================================================== -->
<!-- Bread crumb and right sidebar toggle -->
<!-- ============================================================== -->
<div class="page-breadcrumb">
    <div class="row">
        <div class="col-5 align-self-center">
            <h4 class="page-title" data-i18n="Jspot.Home.Title"></h4>
            <div class="d-flex align-items-center">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item active" aria-current="page" data-i18n="Jspot.Home.Breadcumb1"></li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<br />
<!-- ============================================================== -->
<!-- Container fluid  -->
<!-- ============================================================== -->
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="">
                    <div class="row">
                        <div class="col-lg-3 border-right pr-0">
                            <div class="card-body border-bottom">
                                <h4 class="card-title mt-2" data-i18n="Jspot.Home.PanelTitle"></h4>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div id="calendar-events" class="" data-elysium-jspot-home-listinvitations>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-9">
                            <div class="card-body b-l calender-sidebar">
                                <div id="eventCalendar"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ============================================================== -->
<!-- Modal  -->
<!-- ============================================================== -->
<div id="modalEvent" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myLargeModalLabel" data-i18n="Jspot.Home.ModalEventTitle"></h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">

                <!-- Begin wizard form -->
                <form id="EventForm" name="EventForm">
                    <div id="EventWizard">
                        <!-- Begin wizard-step -->
                        <ul>
                            <li class="col-md-3 col-sm-4 col-6">
                                <a href="#step-1">
                                    <span class="number">1</span>
                                    <span class="info text-ellipsis" data-i18n="[html]Jspot.Home.Wizard.Step1Title"></span>
                                </a>
                            </li>
                            <li class="col-md-3 col-sm-4 col-6">
                                <a href="#step-2">
                                    <span class="number">2</span>
                                    <span class="info text-ellipsis" data-i18n="[html]Jspot.Home.Wizard.Step2Title"></span>
                                </a>
                            </li>
                            <li class="col-md-3 col-sm-4 col-6">
                                <a href="#step-3">
                                    <span class="number">3</span>
                                    <span class="info text-ellipsis" data-i18n="[html]Jspot.Home.Wizard.Step3Title"></span>
                                </a>
                            </li>
                            <li class="col-md-3 col-sm-4 col-6">
                                <a href="#step-4">
                                    <span class="number">4</span>
                                    <span class="info text-ellipsis" data-i18n="[html]Jspot.Home.Wizard.Step4Title"></span>
                                </a>
                            </li>
                            <li class="col-md-3 col-sm-4 col-6">
                                <a href="#step-5">
                                    <span class="number">5</span>
                                    <span class="info text-ellipsis" data-i18n="[html]Jspot.Home.Wizard.Step5Title"></span>
                                </a>
                            </li>
                        </ul>
                        <!-- Begin wizard steps content -->
                        <div>
                            <div id="step-1">
                                <!-- Name* -->
                                <div class="row">
                                    <div class="col-md-6 mb-3">
                                        <label for="Event_Name" data-i18n="Jspot.Home.Wizard.Step1.Name"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Event_Name"
                                               name="Event_Name"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-1"
                                               required
                                               data-parsley-required-message=""
                                               data-parsley-minlength="5"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="150"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step1.RequiredName;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step1.MinLenghtName;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step1.MaxLenghtName;"
                                               data-elysium-iform-uppercase />
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-name></p>
                                    </div>
                                </div>
                                <!-- *Name -->
                                <!-- Description* -->
                                <div class="row">
                                    <div class="col-md-6 mb-3">
                                        <label for="Event_Name" data-i18n="Jspot.Home.Wizard.Step1.Description"></label>
                                        <textarea class="form-control ryusei-text-uppercase" style="resize: none" rows="5"
                                                  id="Event_Description"
                                                  name="Event_Description"
                                                  data-parsley-trigger="change"
                                                  data-parsley-group="step-1"
                                                  required
                                                  data-parsley-required-message=""
                                                  data-parsley-minlength="15"
                                                  data-parsley-minlength-message=""
                                                  data-parsley-maxlength="250"
                                                  data-parsley-maxlength-message=""
                                                  data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step1.RequiredDescription;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step1.MinLenghtDescription;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step1.MaxLenghtDescription;"
                                                  data-elysium-iform-uppercase></textarea>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-description></p>
                                    </div>
                                </div>
                                <!-- *Description -->
                                <!-- Dates* -->
                                <div class="row">
                                    <div class="col-md-6">
                                        <label for="Event_StartDate" data-i18n="Jspot.Home.Wizard.Step1.StartDate"></label>
                                        <input class="form-control"
                                               id="Event_StartDate"
                                               name="Event_StartDate"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-1"
                                               required
                                               data-parsley-required-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step1.RequiredStartDate;" />
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-startdate></p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <label for="Event_EndDate" data-i18n="Jspot.Home.Wizard.Step1.EndDate"></label>
                                        <input class="form-control"
                                               id="Event_EndDate"
                                               name="Event_EndDate"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-1"
                                               required
                                               data-parsley-required-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step1.RequiredEndDate;" />
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-enddate></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- *Dates -->
                            </div>
                            <!-- =================================================== -->
                            <!--                        Address                     -->
                            <!-- =================================================== -->
                            <div id="step-2">

                                <!-- Street* -->
                                <div class="row">
                                    <div class="col-md-6 mb-3">
                                        <label for="Address_Street" data-i18n="Jspot.Home.Wizard.Step2.Street"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Address_Street"
                                               name="Address_Street"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-2"
                                               required
                                               data-parsley-required-message=""
                                               data-parsley-minlength="5"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="50"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step2.RequiredStreet;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step2.MinLenghtStreet;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step2.MaxLenghtStreet;"
                                               data-elysium-iform-uppercase />
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-street></p>
                                    </div>
                                </div>
                                <!-- *Street -->
                                <!-- Numbesr* -->
                                <div class="row">
                                    <div class="col-md-6 mb-3">
                                        <label for="Address_Street" data-i18n="Jspot.Home.Wizard.Step2.ExternalNumber"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Address_ExternalNumber"
                                               name="Address_ExternalNumber"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-2"
                                               required
                                               data-parsley-required-message=""
                                               data-parsley-minlength="1"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="10"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step2.RequiredExternalNumber;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step2.MinLenghtExternalNumber;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step2.MaxLenghtExternalNumber;"
                                               data-elysium-iform-uppercase />
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-externalnumber></p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-3">
                                        <label for="Address_Street" data-i18n="Jspot.Home.Wizard.Step2.InternalNumber"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Address_InternalNumber"
                                               name="Address_InternalNumber"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-2"
                                               data-parsley-minlength="1"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="10"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-minlength-message]Jspot.Home.Wizard.Step2.MinLenghtInternalNumber;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step2.MaxLenghtInternalNumber;"
                                               data-elysium-iform-uppercase />
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-internalnumber></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- *Numbers -->
                                <!-- Street* -->
                                <div class="row">
                                    <div class="col-md-8 mb-3">
                                        <label for="Address_Neighborhood" data-i18n="Jspot.Home.Wizard.Step2.Neighborhood"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Address_Neighborhood"
                                               name="Address_Neighborhood"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-2"
                                               required
                                               data-parsley-required-message=""
                                               data-parsley-minlength="5"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="100"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step2.RequiredNeighborhood;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step2.MinLenghtNeighborhood;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step2.MaxLenghtNeighborhood;"
                                               data-elysium-iform-uppercase />
                                    </div>
                                    <div class="col-md-4 mb-3">
                                        <label for="Address_ZipCode" data-i18n="Jspot.Home.Wizard.Step2.ZipCode"></label>
                                        <input type="text" class="form-control ryusei-text-uppercase"
                                               id="Address_ZipCode"
                                               name="Address_ZipCode"
                                               data-parsley-trigger="change"
                                               data-parsley-group="step-2"
                                               required
                                               data-parsley-required-message=""
                                               data-parsley-minlength="5"
                                               data-parsley-minlength-message=""
                                               data-parsley-maxlength="10"
                                               data-parsley-maxlength-message=""
                                               data-i18n="[data-parsley-required-message]Jspot.Home.Wizard.Step2.RequiredZipCode;
                                                          [data-parsley-minlength-message]Jspot.Home.Wizard.Step2.MinLenghtZipCode;
                                                          [data-parsley-maxlength-message]Jspot.Home.Wizard.Step2.MaxLenghtZipCode;"
                                               data-elysium-iform-uppercase />
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-street></p>
                                    </div>
                                </div>
                                <!-- *Street -->
                                <!-- World* -->
                                <div class="row">
                                    <!-- Country -->
                                    <div class="col-md-4 mb-3">
                                        <label for="Address_Country" data-i18n="Jspot.Home.Wizard.Step2.Country"></label>
                                        <select class="form-control ryusei-text-uppercase"
                                                id="Address_Country"
                                                name="Address_Country"
                                                data-parsley-trigger="change"
                                                data-parsley-group="step-2"
                                                data-parsley-select-required
                                                data-parsley-select-required-message=""
                                                data-i18n="[data-parsley-select-required-message]Jspot.Home.Wizard.Step2.RequiredCountry;"
                                                data-elysium-iform-uppercase>

                                            <option value="mx" selected>México</option>

                                        </select>
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-country></p>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- State -->
                                    <div class="col-md-4 mb-3">
                                        <label for="Address_State" data-i18n="Jspot.Home.Wizard.Step2.State"></label>
                                        <select class="form-control ryusei-text-uppercase"
                                                id="Address_State"
                                                name="Address_State"
                                                data-parsley-trigger="change"
                                                data-parsley-group="step-2"
                                                data-parsley-select-required
                                                data-parsley-select-required-message=""
                                                data-i18n="[data-parsley-select-required-message]Jspot.Home.Wizard.Step2.RequiredState;"
                                                data-elysium-iform-uppercase>
                                            <option value="default" selected>Seleccionar Estado</option>
                                            <option value="Ciudad de Mexico">Ciudad de Mexico</option>
                                            <option value="Estado de Aguascalientes">Estado de Aguascalientes</option>
                                            <option value="Estado de Baja California">Estado de Baja California</option>
                                            <option value="Estado de Baja California Sur">Estado de Baja California Sur</option>
                                            <option value="Estado de Campeche">Estado de Campeche</option>
                                            <option value="Estado de Chiapas">Estado de Chiapas</option>
                                            <option value="Estado de Chihuahua">Estado de Chihuahua</option>
                                            <option value="Estado de Coahuila de Zaragoza">Estado de Coahuila de Zaragoza</option>
                                            <option value="Estado de Colima">Estado de Colima</option>
                                            <option value="Estado de Durango">Estado de Durango</option>
                                            <option value="Estado de Guanajuato">Estado de Guanajuato</option>
                                            <option value="Estado de Guerrero">Estado de Guerrero</option>
                                            <option value="Estado de Hidalgo">Estado de Hidalgo</option>
                                            <option value="Estado de Jalisco">Estado de Jalisco</option>
                                            <option value="Estado de Mexico">Estado de Mexico</option>
                                            <option value="Estado de Michoacan de Ocampo">Estado de Michoacan de Ocampo</option>
                                            <option value="Estado de Morelos">Estado de Morelos</option>
                                            <option value="Estado de Nayarit">Estado de Nayarit</option>
                                            <option value="Estado de Nuevo Leon">Estado de Nuevo Leon</option>
                                            <option value="Estado de Oaxaca">Estado de Oaxaca</option>
                                            <option value="Estado de Puebla">Estado de Puebla</option>
                                            <option value="Estado de Queretaro">Estado de Queretaro</option>
                                            <option value="Estado de Quintana Roo">Estado de Quintana Roo</option>
                                            <option value="Estado de San Luis Potosi">Estado de San Luis Potosi</option>
                                            <option value="Estado de Sinaloa">Estado de Sinaloa</option>
                                            <option value="Estado de Sonora">Estado de Sonora</option>
                                            <option value="Estado de Tabasco">Estado de Tabasco</option>
                                            <option value="Estado de Tamaulipas">Estado de Tamaulipas</option>
                                            <option value="Estado de Tlaxcala">Estado de Tlaxcala</option>
                                            <option value="Estado de Veracruz-Llave">Estado de Veracruz-Llave</option>
                                            <option value="Estado de Yucatan">Estado de Yucatan</option>
                                            <option value="Estado de Zacatecas">Estado de Zacatecas</option>
                                        </select>
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-state></p>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- City -->
                                    <div class="col-md-4 mb-3">
                                        <label for="Address_City" data-i18n="Jspot.Home.Wizard.Step2.City"></label>
                                        <select class="form-control ryusei-text-uppercase"
                                                id="Address_City"
                                                name="Address_City"
                                                data-parsley-trigger="change"
                                                data-parsley-group="step-2"
                                                data-parsley-select-required
                                                data-parsley-select-required-message=""
                                                data-i18n="[data-parsley-select-required-message]Jspot.Home.Wizard.Step2.RequiredCity;"
                                                data-elysium-iform-uppercase></select>
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-city></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- *World -->
                                <!-- Maps* -->
                                <div class="row">
                                    <div class="col-md-12 mb-3">
                                        <label for="Address_City" data-i18n="Jspot.Home.Wizard.Step2.Map"></label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 mb-3">
                                        <div id="EventAddressMap" class="gmaps" style="height:300px"></div>
                                        <div class="row" style="margin-bottom: 0">
                                            <div class="col-md-12">
                                                <select id="Address_Marker"
                                                        name="Address_Marker"
                                                        data-parsley-trigger="change"
                                                        data-parsley-group="step-2"
                                                        data-parsley-marker-set
                                                        data-parsley-marker-set-message=""
                                                        data-i18n="[data-parsley-marker-set-message]Jspot.Home.Wizard.Step2.RequiredMarker;"
                                                        style="display:none">
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                </select>
                                                <p style="margin-bottom: 0" data-elysium-jspot-home-event-marker></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- *Maps -->
                            </div>
                            <div id="step-3">
                                <!-- Button to create dynamic elements -->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="btn-group pull-right">
                                            <button data-elysium-jspot-home-event-add-department type="button" class="btn btn-default btn-icon"><span class="fas fa-plus"></span></button>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <!-- List of elements -->
                                <div class="row">
                                    <div class="col-md-12" data-elysium-jspot-home-event-list-departments></div>
                                </div>
                                <!-- Validation of empty list -->
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <select id="CollectionDepartment_List"
                                                name="CollectionDepartment_List"
                                                data-parsley-trigger="change"
                                                data-parsley-group="step-3"
                                                data-parsley-department-one
                                                data-parsley-department-one-message=""
                                                data-i18n="[data-parsley-department-one-message]Jspot.Home.Wizard.Step3.DepartmentAtLeastOne;"
                                                style="display:none">
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                        </select>
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-department-list-error></p>
                                    </div>
                                </div>
                            </div>
                            <div id="step-4">
                                <!-- Button to create dynamic elements -->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="btn-group pull-right">
                                            <button data-elysium-jspot-home-event-add-eventgroup type="button" class="btn btn-default btn-icon"><span class="fas fa-plus"></span></button>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <!-- List of elements -->
                                <div class="row">
                                    <div class="col-md-12" data-elysium-jspot-home-event-list-eventgroup></div>
                                </div>
                                <!-- Validation of empty list -->
                                <div class="row" style="margin-bottom: 0">
                                    <div class="col-md-12">
                                        <select id="CollectionEventGroup_List"
                                                name="CollectionEventGroup_List"
                                                data-parsley-trigger="change"
                                                data-parsley-group="step-4"
                                                data-parsley-eventgroup-one
                                                data-parsley-eventgroup-one-message=""
                                                data-i18n="[data-parsley-eventgroup-one-message]Jspot.Home.Wizard.Step4.GroupAtLeastOne;"
                                                style="display:none">
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                        </select>
                                        <p style="margin-bottom: 0" data-elysium-jspot-home-event-event-group-error></p>
                                    </div>
                                </div>
                            </div>
                            <div id="step-5">
                                <!-- List of elements -->
                                <div class="row">
                                    <div class="col-md-12" data-elysium-jspot-home-event-eventdepartment-list></div>
                                </div>
                                <br />
                                <!-- List of elements -->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="btn-group pull-right">
                                            <button type="submit" class="btn btn-primary" data-i18n="[html]Jspot.Home.Wizard.Step5.CreateEvent"></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End wizard steps content -->
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

<div style="display:none" data-elysium-jspot-home-event-department-template>
